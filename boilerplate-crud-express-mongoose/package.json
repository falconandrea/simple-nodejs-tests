{
  "name": "boilerplate-crud-express-mongoose",
  "version": "1.0.0",
  "description": "Simple boilerplate for CRUD with Express and Moongose",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write .",
    "eslint": "eslint --fix .",
    "start": "NODE_ENV=development node index.js",
    "test": "NODE_ENV=test jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "boilerplate",
    "crud",
    "express",
    "moongose",
    "nodejs",
    "mongodb"
  ],
  "author": "Andrea Falcon <falcon.andrea88@gmail.com> (https://andreafalcon.dev/)",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.20.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "jest": "^29.0.1",
    "mongoose": "^6.5.3"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.1.0",
    "prettier": "^2.7.1",
    "supertest": "^6.2.4"
  }
}
